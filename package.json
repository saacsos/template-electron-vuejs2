{
    "name": "template-electron-vuejs2",
    "version": "1.0.0",
    "description": "electron template with vuejs 2 , jquery 3, firebase 3",
    "main": "main.js",
    "scripts": {
        "compile": "webpack",
        "start": "electron main.js",
        "build-dev": "electron-packager . appname",
        "build": "electron-packager . appname --ignore-node_modules/electron-* --asar",
        "build-all": "electron-packager . appname --ignore-node_modules/electron-* --asar --all"
    },
    "keywords": [
        "electron", "vuejs2", "jquery3", "firebase3"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "asar": "^0.13.0",
        "babel-core": "^6.24.1",
        "babel-loader": "^7.0.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-runtime": "^5.8.38",
        "css-loader": "^0.28.1",
        "electron": "^7.2.4",
        "electron-packager": "^8.7.0",
        "vue-hot-reload-api": "^2.1.0",
        "vue-html-loader": "^1.2.4",
        "vue-loader": "^12.0.3",
        "vue-style-loader": "^3.0.1",
        "vue-template-compiler": "^2.3.2",
        "webpack": "^2.5.0",
        "webpack-dev-server": "^3.1.11"
    },
    "dependencies": {
        "firebase": "^3.9.0",
        "jquery": "^3.7.1",
        "vue": "^2.3.2"
    }
}
